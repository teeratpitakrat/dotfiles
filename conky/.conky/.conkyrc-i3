out_to_x no
own_window no
out_to_console yes
double_buffer no
background no
update_interval 1
total_run_times 0
short_units yes
if_up_strictness address
use_spacer right
override_utf8_locale yes
use_xft yes
cpu_avg_samples 2
pad_percents 2
top_name_width 8

TEXT
[\
{"full_text":\
	${if_match ${cpu}>=80}\
		"Top Cpu: ${top name 1},${top name 2},${top name 3}"\
	${else}\
		""\
	${endif},\
"color" :\
	${if_match ${cpu}>=80}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	${if_match ${memperc}>=70}\
		${if_match ${swapperc}>=10}\
			"Top Mem: ${top_mem name 1},${top_mem name 2},${top_mem name 3}"\
		${else}\
			""\
		${endif}\
	${else}\
		""\
	${endif},\
"color" :\
	${if_match ${memperc}>=80}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	"${loadavg}",\
"color" :\
	${if_match ${loadavg 1}>=4}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	${if_match "${nodename}"=="apollo"}\
		"CPU: ${cpu}% ${texeci 5 sensors | grep 'temp1' | cut -c16-17 }°C"\
	${else}\
		${if_match "${nodename}"=="zeus"}\
			"CPU: ${cpu}% ${texeci 5 sensors | grep 'Physical id 0' | cut -c18-19 }°C"\
		${else}\
			""\
		${endif}\
	${endif},\
"color" :\
	${if_match "${nodename}"=="apollo"}\
		${if_match ${texeci 5 sensors | grep 'temp1' | cut -c16-17 }>70}\
			"\#ff0000"\
		${else}\
			"\#769ebb"\
		${endif}\
	${else}\
		${if_match "${nodename}"=="zeus"}\
			${if_match ${texeci 5 sensors | grep 'Physical id 0' | cut -c18-19 }>70}\
				"\#ff0000"\
			${else}\
				"\#769ebb"\
			${endif}\
		${else}\
			"\#769ebb"\
		${endif}\
	${endif}\
	},\
{"full_text":\
	"Mem: ${memperc}% ${mem}",\
"color" :\
	${if_match ${memperc}>80}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	"Swap: ${swapperc}% ${swap}" ,\
"color" :\
	${if_match ${memperc}>=70}\
		${if_match ${swapperc}=>20}\
			"\#ff0000"\
		${else}\
			"\#769ebb"\
		${endif}\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	"/${fs_free /}",\
"color" :\
	${if_match ${fs_used_perc /}>75}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	},\
{"full_text":\
	"${if_mounted /home}${fs_free /home}",\
"color" :\
	${if_match ${fs_used_perc /}>75}\
		"\#ff0000"\
	${else}\
		"\#769ebb"\
	${endif}\
	$endif\
	},\
{"full_text":\
	"${if_mounted /media/mybook1tb}M1 ${fs_free /media/mybook1tb}${endif}",\
	"color" :\
	${if_match ${fs_used_perc /media/mybook1tb}>75}"\#ff0000"${else}"\#769ebb"${endif}\
	},\
{"full_text":\
	"${if_mounted /media/mybook2tb}M2 ${fs_free /media/mybook2tb}${endif}",\
	"color" :\
	${if_match ${fs_used_perc /media/mybook2tb}>75}"\#ff0000"${else}"\#769ebb"${endif}\
	},\
{"full_text":\
	"${if_mounted /media/brisbane}B ${fs_free /media/brisbane}${endif}",\
	"color" :\
	${if_match ${fs_used_perc /media/brisbane}>75}"\#ff0000"${else}"\#769ebb"${endif}\
	},\
{"full_text":\
	${if_match "${nodename}"=="apollo"}\
		"${battery BAT1} ${battery_time BAT1}"\
	${endif}\
	${if_match "${nodename}"=="zeus"}\
		"${battery BAT0} ${battery_time BAT0}"\
	${endif},\
"color" :\
	${if_match "${nodename}"=="apollo"}\
		${if_match ${battery_percent BAT1}<50}\
			"\#ff0000"\
		${else}\
			"\#769ebb"\
		${endif}\
	${endif}\
	${if_match "${nodename}"=="zeus"}\
		${if_match ${battery_percent BAT0}<30}\
			"\#ff0000"\
		${else}\
			"\#769ebb"\
		${endif}\
	${endif}\
	},\
{"full_text":\
	"${time %a %d.%m.%y} ${time %H:%M:%S}",\
"color" :\
	${if_match ${time %H}<4}\
		"\#ff0000"${else}"\#eeeeee"\
	${endif}\
	}\
],
